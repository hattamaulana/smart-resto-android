<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/home_navigation"
    app:startDestination="@id/splashScreenDest">

    <fragment
        android:id="@+id/splashScreenDest"
        android:name="id.ac.polinema.seameo.ecanteen.view.fragment.SplashScreen"
        android:label="SplashScreen"
        tools:layout="@layout/fragment_splashscreen">
        <action
            android:id="@+id/toHome"
            app:destination="@id/homeDest" />
    </fragment>

    <fragment
        android:id="@+id/homeDest"
        android:name="id.ac.polinema.seameo.ecanteen.view.fragment.HomeFragment"
        android:label="HOME"
        tools:layout="@layout/fragment_home"></fragment>
</navigation>